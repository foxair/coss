enniu.version								= 1.0.0

#mode
enniu.basic.mode							= dev

#enniu-basic
enniu.basic.workdir							= ${enniu_basic_workdir}
enniu.basic.output							= ${enniu_basic_workdir}/output
enniu.basic.deploy							= ${enniu_basic_workdir}/target/deploy
enniu.basic.logs							= ${enniu_basic_output}/logs
enniu.basic.apache.port						= 80
enniu.basic.appserver.port					= 8080
enniu.basic.debug.port						= 8090
enniu.basic.domain							= binq.com
enniu.basic.javahome						= E:/Java/JDK/jdk6
enniu.basic.jbosshome						= D:/Server/jboss-4.0.5.GA
enniu.basic.apachehome						= D:/Server/httpd
enniu.basic.apache.group					= systems
enniu.basic.apache.user						= admin

#enniu-db-mysql
enniu.app.db.mysql.driver					= ${enniu_app_db_mysql_driver}
enniu.app.db.mysql.jdbcUrl					= ${enniu_app_db_mysql_jdbcUrl}
enniu.app.db.mysql.username					= ${enniu_app_db_mysql_username}
enniu.app.db.mysql.password					= ${enniu_app_db_mysql_password}
#enniu-db-pool
enniu.app.db.pool.initialSize				= ${enniu_app_db_pool_initialSize}
enniu.app.db.pool.minIdle					= ${enniu_app_db_pool_minIdle}
enniu.app.db.pool.maxActive					= ${enniu_app_db_pool_maxActive}
enniu.app.db.pool.maxWait					= ${enniu_app_db_pool_maxWait}
enniu.app.db.pool.poolPreparedStatements	= ${enniu_app_db_pool_poolPreparedStatements}
enniu.app.db.pool.filters					= ${enniu_app_db_pool_filters}

#enniu-web
enniu.web.bundle							= ${enniu_basic_workdir}/web/home
enniu.web.templates							= ${enniu_web_bundle}/WEB-INF/template