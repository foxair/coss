coss.version								= 1.0.0

#mode
coss.basic.mode								= dev

#coss-basic
coss.basic.workdir							= ${coss_basic_workdir}
coss.basic.output							= ${coss_basic_workdir}/output
coss.basic.deploy							= ${coss_basic_workdir}/target/deploy
coss.basic.logs								= ${coss_basic_output}/logs
coss.basic.apache.port						= 80
coss.basic.appserver.port					= 8080
coss.basic.debug.port						= 8090
coss.basic.domain							= binq.com
coss.basic.javahome							= E:/Java/JDK/jdk6
coss.basic.jbosshome						= D:/Server/jboss-4.0.5.GA
coss.basic.apachehome						= D:/Server/httpd
coss.basic.apache.group						= systems
coss.basic.apache.user						= admin

#coss-db-mysql
coss.biz.db.mysql.driver					= ${coss.biz.db.mysql.driver}
coss.biz.db.mysql.jdbcUrl					= ${coss.biz.db.mysql.jdbcUrl}
coss.biz.db.mysql.username						= ${coss.biz.db.mysql.name}
coss.biz.db.mysql.password					= ${coss.biz.db.mysql.password}
#coss-db-pool
coss.biz.db.pool.initialPoolSize			= ${coss.biz.db.pool.initialPoolSize}
coss.biz.db.pool.minPoolSize				= ${coss.biz.db.pool.minPoolSize}
coss.biz.db.pool.maxPoolSize				= ${coss.biz.db.pool.maxPoolSize}
coss.biz.db.pool.checkoutTimeout			= ${coss.biz.db.pool.checkoutTimeout}
coss.biz.db.pool.maxIdleTime				= ${coss.biz.db.pool.maxIdleTime}
coss.biz.db.pool.maxActiveTime				= ${coss.biz.db.pool.maxActiveTime}
coss.biz.db.pool.idleConnectionTestPeriod	= ${coss.biz.db.pool.idleConnectionTestPeriod}
coss.biz.db.pool.acquireIncrement			= ${coss.biz.db.pool.acquireIncrement}

#coss-web-home
coss.web.home								= ${coss_basic_workdir}/web/home
coss.web.home.template						= ${coss_web_home}/WEB-INF/template