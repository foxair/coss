coss.version								= 1.0.0

#mode
coss.basic.mode							= dev

#coss-basic
coss.basic.workdir							= ${coss_basic_workdir}
coss.basic.output							= ${coss_basic_workdir}/output
coss.basic.deploy							= ${coss_basic_workdir}/target/deploy
coss.basic.logs							= ${coss_basic_output}/logs
coss.basic.apache.port						= 80
coss.basic.appserver.port					= 8080
coss.basic.debug.port						= 8090
coss.basic.domain							= binq.com
coss.basic.javahome						= E:/Java/JDK/jdk6
coss.basic.jbosshome						= D:/Server/jboss-4.0.5.GA
coss.basic.apachehome						= D:/Server/httpd
coss.basic.apache.group					= systems
coss.basic.apache.user						= admin

#coss-db-mysql
coss.app.db.mysql.driver					= ${coss_app_db_mysql_driver}
coss.app.db.mysql.jdbcUrl					= ${coss_app_db_mysql_jdbcUrl}
coss.app.db.mysql.username					= ${coss_app_db_mysql_username}
coss.app.db.mysql.password					= ${coss_app_db_mysql_password}
#coss-db-pool
coss.app.db.pool.initialSize				= ${coss_app_db_pool_initialSize}
coss.app.db.pool.minIdle					= ${coss_app_db_pool_minIdle}
coss.app.db.pool.maxActive					= ${coss_app_db_pool_maxActive}
coss.app.db.pool.maxWait					= ${coss_app_db_pool_maxWait}
coss.app.db.pool.poolPreparedStatements	= ${coss_app_db_pool_poolPreparedStatements}
coss.app.db.pool.filters					= ${coss_app_db_pool_filters}

#coss-web
coss.web.bundle							= ${coss_basic_workdir}/web/home
coss.web.templates							= ${coss_web_bundle}/WEB-INF/template